    <script type="text/javascript">
    
    function initMap(double lat, double lng) {
    	var map;
    	map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: lat, lng: lng},
        zoom: 11
      });
    }
    function drowMap() {
    	var map;
    	map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: 37.5554367, lng: 126.9689898},
        zoom: 11
      });
    </script>
