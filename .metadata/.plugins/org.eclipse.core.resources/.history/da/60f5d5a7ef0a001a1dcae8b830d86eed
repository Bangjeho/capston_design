    <script type="text/javascript">
    function initMap(latitude, longitude) {
    	var map;
    	var geometry = {lat: latitude, lng: longitude};
    	map = new google.maps.Map(document.getElementById('map'), {
        center: geometry,
        zoom: 12
        });
        var marker = new google.maps.Marker({
            position: geometry,
            map: map,
            title: 'Hello World!'
        });
        

        var icons = {
        price_one_fif: {
            icon: "image/yellopoint.png"
          },
        price_fif_oneh: {
            icon: "image/cyonpoint.png"
        },
        price_oneh_onehf: {
            icon: "image/bluepoint.png"
        },
        price_onehf_twoh: {
            icon: "image/peoplepoint.png"
        },
        price_twoh_fih: {
            icon: "image/pinkpoint.png"
        },
        price_fih_ones: {
            icon: "image/orrangepoint.png"
        }
        };
        var test = [[37.5397092,126.9438513],[37.5305535,126.9634576]];

        for(int i ; i <test.length; i++){
        	var features = [
                       	 {
                         	 position: new google.maps.LatLng(test[i]),
                         	 type: 'price_one_fif'
                        	}
                       	 ];
        
        	var marker = new google.maps.Marker({
        		position : features[0].position,
        		icon: icons[features[0].type].icon,
        		map : map
        });
    };
    }
    </script>
