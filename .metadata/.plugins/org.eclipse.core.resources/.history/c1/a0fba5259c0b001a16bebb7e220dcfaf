function initMap(latitude, longitude, paramGeos, group) 
{
       var geometry = {lat: latitude, lng: longitude};
       var map = new google.maps.Map(document.getElementById('map'), {
        center: geometry,
        zoom: 18
        });
       	 var groups = group;
         var geos = paramGeos;
         var marker = new google.maps.Marker({
             position: geometry,
             map: map,
             title : group[1]
            	 });
         
        setMarkers(map, geos,groups);
      
}


    
    function setMarkers(map, geos,groups){
       var icons = {
               group_1: {
                   icon: 'image/yellowpoint.png',
                 },
               group_2: {
                   icon: 'image/cyanpoint.png',
               },
               group_3: {
                   icon: 'image/bluepoint.png',
               },
               group_4: {
                   icon: 'image/purplepoint.png',
               },
               group_5: {
                   icon: 'image/pinkpoint.png',
               },
               group_6: {
                   icon: 'image/orangepoint.png',
               }};


        for(var i = 0; i<geos.length ; i++){
           var geo = geos[i];
           var group = groups[i];
           var marker = new google.maps.Marker({
              position : {lat : geo[0],lng : geo[1]},
              map : map,
              icon: icons[group].icon
        });
    }
    }