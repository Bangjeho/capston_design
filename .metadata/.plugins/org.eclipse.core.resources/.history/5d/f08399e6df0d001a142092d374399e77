package kr.ac.sunmoon.client;

import com.google.gwt.dom.client.Style.Unit;
import com.google.gwt.user.client.ui.HorizontalPanel;
import com.google.gwt.user.client.ui.Image;

public class ImagePanel extends HorizontalPanel{
	public ImagePanel(){
		super();
		Image img = new Image();
		img.setUrl("image/airbnb-image.png");
		img.setWidth("1500px");
		img.getElement().getStyle().setMarginBottom(600, Unit.PX);
		this.add(img);
	}
}
